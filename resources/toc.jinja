<!doctype html>
<html>
<head>
    <title>Table of Contents</title>
    <meta http-equiv="content-type" content="text/html; charset=iso-8859-1">
    <link rel="stylesheet" href="style.css">
</head>
<body>
<div id="toc">
    {% for section_name, section in sections.items() -%}
        <a style="text-decoration: none !important" href="economist.html#{{ section['id'] }}"><h2>{{ section_name }}</h2></a>
        <ul>
        {% for article in section['articles'] -%}
            <li><div><a style="text-decoration: none !important" href="economist.html#{{ article['id'] }}">{{ article['title'] }}</a></div></li>
        {% endfor -%}
        </ul>
    {% endfor -%}
</div>
</body>
</html>
